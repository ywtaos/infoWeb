<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="chinsoft.entity.Favorite" table="Favorite">
       <id name="ID" column="ID" type="string">
            <generator class="uuid"/>
        </id>
       <property name="ProductID" column="ProductID" type="string"/>
       <property name="PubMemberID" column="PubMemberID" type="string"/>
       <property name="PubDate" column="PubDate" type="date"/>
    </class>
</hibernate-mapping>