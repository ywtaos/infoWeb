<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="chinsoft.entity.Rate" table="Rate">
       <id name="ID" column="ID" type="string">
            <generator class="uuid"/>
        </id>
       <property name="PubMemberID" column="PubMemberID" type="string"/>
       <property name="ToMemberID" column="ToMemberID" type="string"/>
       <property name="StarID" column="StarID" type="integer"/>
       <property name="PubDate" column="PubDate" type="date"/>
       <property name="Comment" column="Comment" type="string"/>
    </class>
</hibernate-mapping>
